<!DOCTYPE html>
<html>
<head>
  <title>MusicApp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  <% if flash[:errors] %>
    <%= flash[:errors] %>
  <% end %>
  
<%= yield %>


<% if current_user %>
  <%= button_to "Logout", session_url, method: :delete%>
<% end %>

</body>
</html>
