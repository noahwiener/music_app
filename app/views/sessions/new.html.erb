
<h1>Log In</h1>

<form action="/session" method="post">
  <%= auth_token %>

  <label for="email">Email</label>
  <br>
  <input type="text" name="user[email]">
  <br>

  <label for="password">Password</label>
  <br>
  <input type="password" name="user[password]">
  <br>
  <br>

  <input type="submit">
</form>
